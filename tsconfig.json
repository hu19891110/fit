{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "target": "es6",
        "removeComments": true,
        "preserveConstEnums": true,
        "experimentalDecorators": true,
        "jsx": "react",
        "sourceMap": true
    },
    "filesGlob": [
        "*.d.ts"
    ],
    "exclude": [
        "node_modules",
        "output",
        "typings",
        "test_coverage_html",
        "test_jasmine_html"
    ]
}